    "rxjs": "^5.2.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.37",
    "@types/redux-logger": "^2.6.31",
    "@types/redux-thunk": "^2.1.30",
    "@types/sinon": "^1.16.29",
    "@types/sinon-chai": "^2.7.26",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.0.2",
    "mocha-cli": "^1.0.1",
